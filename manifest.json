{
  "manifest_version": 3,
  "name": "Oryx Solver - An Ai Homework Helper",
  "version": "0.1",
  "description": "an ai homework helper",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "34": "icons/64.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "side_panel": {
    "default_path": "src/sidepanel/index.html"
  },
  "permissions": [
    "sidePanel",
    "storage",
    "activeTab"
  ]
}
