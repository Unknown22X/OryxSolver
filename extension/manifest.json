{
  "key": "",
  "manifest_version": 3,
  "name": "Oryx Solver - An Ai Homework Helper",
  "version": "0.1",
  "description": "an ai homework helper",
  "icons": {
    "16": "public/icons/16.png",
    "32": "public/icons/32.png",
    "34": "public/icons/34.png",
    "48": "public/icons/48.png",
    "128": "public/icons/128.png"
  },
  "action": {},
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src https://equal-moth-95.clerk.accounts.dev https://clerk-telemetry.com https://*.supabase.co https://api.polar.sh;"
  },
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  },
  "side_panel": {
    "default_path": "src/sidepanel/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content/cropOverlay.ts"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "sidePanel",
    "storage",
    "cookies",
    "activeTab",
    "tabs",
    "scripting"
  ], 
  "host_permissions": [
    "<all_urls>",
    "https://equal-moth-95.clerk.accounts.dev/*",
    "https://clerk-telemetry.com/*",
    "http://localhost/*",
    "https://*.supabase.co/*",
    "https://api.polar.sh/*", 
    "https://MyVerySecretWebApp.com/*"
  ]
}
